{
    "compilerOptions": {
        "lib": ["es2023"],
        "module": "node16",
        "moduleResolution": "node16",
        "target": "es6",
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "noEmit": true,
        "verbatimModuleSyntax": false
    },
    "include": ["./src/**/*.ts", "./launcher/**/*.ts", "./utils/**/*.ts", "./webpack.config.ts"]
}
